  <Character ID="R_Corp_Rudolph_2_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Geyikler, çabucak süpürüp geçelim onları.</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">Bunu zaten bir kez yaşadık, değil mi?</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">Bu tahmin edilebilir bir stratejiydi. Bizim için hızlı bir zafer.</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">Bizi bir daha köşeye sıkıştırmalarına izin veremem…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">Sınırıma yaklaştım… Bunu hızlıca bitirmeliyim…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">Her şey zihinsel beceri meselesi…</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">Çok dikkatsizdin sanırım. Bunun geçen seferkiyle aynı olacağını düşündün.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">Geyikler. İlerliyoruz.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">Acele edin! Eğer kavga uzarsa dezavantajlı duruma düşeriz.</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Reindeer_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Uf… Bir operasyon daha.</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">Biraz şansla bu sefer onları alt ederiz…</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">Gergin olmanın bir anlamı yok. Başka seçeneğimiz yok.</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">Güzel, hayatta kalmak için elimizden geleni yapmalıyız.</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">Hızlıca bitirelim.</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">Bir dakika daha sürseydi kötü bitebilirdi.</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">Yok ediliyoruz değil mi…?</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">Bilmiyorum. Üstler halleder.</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">Geri dönmeli miyiz şimdi, Bay Rudolph?</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">Bu sefer kazanabileceğimi sanmıştım…</BattleDialog>
      <BattleDialog ID="DEATH_1">Umarım diğerleri… bunu halleder…</BattleDialog>
      <BattleDialog ID="DEATH_2">Bu gerçekten son mu…?</BattleDialog>
      <BattleDialog ID="DEATH_3">…Bu canavarları yenmemizin imkanı yoktu…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">Dayanmalıyız… Buna katlanmalıyız.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">Bu, geçen seferki gibi tuhaf bir şekilde tanıdık geliyor…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">Gergin olmamam gerektiğini biliyorum…</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">Nasıl… bu kadar güce sahipler…?</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">İyi bir silah yanlış ellerde işe yaramaz.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">Henüz sorun yaşamıyorum, bu yüzden şimdilik devam edeceğim.</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">… Dayanmak zorlaşıyor. Hızlıca bitirmeliyiz…</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">Görünüşe göre diğerleri tekrar yorulmaya başlıyor.</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Maxim_2_1">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Bu sefer buradan canlı ayrılacağız!</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">Şu anda, her şeyi silip süpürmek görevimiz!</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">Düşüncelerinizi onlarla işiniz bittikten sonraya saklayın!</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">Haha! Bizi küçümsediklerinin karşılığı bu!</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">Geriye kaç tanesi kaldı?</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">İlk seferki kadar zorlular.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">Lanet olsun! O bayağı ezilmiş.</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">Teknikler, ezici güce karşı hiçbir şey ifade etmez!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">İleri atılın ve yol açılacaktır!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">Gergedan'ın boynuzu gibi tek başınıza ilerleyin!</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Rhinoceros_2">
    <Type ID="START_BATTLE">
      <BattleDialog ID="START_BATTLE_0">Cesur ve küstah olun! Gözü pek olun!</BattleDialog>
      <BattleDialog ID="START_BATTLE_1">Düşman kim olursa olsun, onları Gergedan'ın ayakları altında ezeceğim!</BattleDialog>
      <BattleDialog ID="START_BATTLE_2">Şimdi kim ezilmek ister??!</BattleDialog>
      <BattleDialog ID="START_BATTLE_3">Hiçbir şey bir Gergedan'ın yoluna çıkamaz!</BattleDialog>
      <BattleDialog ID="START_BATTLE_4">Hemen başlayalım mı, Yüzbaşı?</BattleDialog>
    </Type>
    <Type ID="BATTLE_VICTORY">
      <BattleDialog ID="BATTLE_VICTORY_0">Kazandık!</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_1">İkinci kez kazanabileceğini mi düşündün? Çok yanlış!</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_2">Bu bizi bir süreliğine yok olmaktan kurtarır.</BattleDialog>
      <BattleDialog ID="BATTLE_VICTORY_3">Çok eğlenceli adamlardı!</BattleDialog>
    </Type>
    <Type ID="DEATH">
      <BattleDialog ID="DEATH_0">Bu bizi yenmek için yeterli olmayacak…!</BattleDialog>
      <BattleDialog ID="DEATH_1">… Uf… Ayakta kalmalıyım…!</BattleDialog>
      <BattleDialog ID="DEATH_2">Böyle düşmeyeceğim bir daha…!</BattleDialog>
      <BattleDialog ID="DEATH_3">Kahretsin… Hiçbir şey değişmemiş…</BattleDialog>
    </Type>
    <Type ID="COLLEAGUE_DEATH">
      <BattleDialog ID="COLLEAGUE_DEATH_0">Bana isabet etseydi, beni de yere serebilirdi.</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_1">Ben buna alışkınım!</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_2">Umarım bu, tüm sürünün yok olmasıyla sonuçlanmaz!</BattleDialog>
      <BattleDialog ID="COLLEAGUE_DEATH_3">Umurumda değil! Yapacak işim var!</BattleDialog>
    </Type>
    <Type ID="KILLS_OPPONENT">
      <BattleDialog ID="KILLS_OPPONENT_0">Çok kırılgan bir vücudun var!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_1">Saldırılarınız işe yaramaz!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_2">Henüz işim bitmedi seninle!</BattleDialog>
      <BattleDialog ID="KILLS_OPPONENT_3">Yoluma çıktın… Ne aptal! Haha!</BattleDialog>
    </Type>
  </Character>
  <Character ID="R_Corp_Myo_2_1">
